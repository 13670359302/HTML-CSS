<view class="contains" style="margin-top: {{ height }}px;">
  <!-- C/XDEL789-456+123=%0.= -->
  <view class="title">计算器</view>
  <input type="number" placeholder="结果" value="{{res}}" />
  <view class="item">
    <button>C</button>
    <button>/</button>
    <button>X</button>
    <button>DEL</button>
  </view>
  <view class="item">
    <button>7</button>
    <button>8</button>
    <button>9</button>
    <button>-</button>
  </view>
  <view class="item">
    <button>4</button>
    <button>5</button>
    <button>6</button>
    <button>+</button>
  </view>
  <view class="item">
    <button>1</button>
    <button>2</button>
    <button>3</button>
    <button style="border-bottom:1px solid transparent">=</button>
  </view>
  <view class="item">
    <button>%</button>
    <button>0</button>
    <button>.</button>
    <button style="border-top:1px solid transparent"></button>
  </view>
</view>